[
  {
    "name": "gog-front-container",
    "image": "321005531610.dkr.ecr.ap-northeast-1.amazonaws.com/gog-front",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/gog-front-ecs-task",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "command": [
      "yarn", "start"
    ],
    "environment": [
      {
        "name": "API_URL",
        "value": "https://geem84.com"
      },
      {
        "name": "CONTAINER_PORT",
        "value": "80"
      },
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "PORT",
        "value": "80"
      },
      {
        "name": "WORKDIR",
        "value": "myapp"
      }
    ]
  }
]